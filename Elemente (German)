      "teuer", "selten", "hitzebeständig", "hohe Dichte", "hohes Gewicht", "stabil", 
      "lässt sich gut legieren"],
      
    };

    const selectedQuestions = new Set();
    const questionsDiv = document.getElementById("questions");
    const resultsDiv = document.getElementById("results");
    const evaluateButton = document.getElementById("evaluate");

    questions.forEach(q => {
      const wrapper = document.createElement("div");
      wrapper.classList.add("question");
      const btn = document.createElement("button");
      btn.innerText = q;
      btn.addEventListener("click", () => {
        btn.classList.toggle("selected");
        if (selectedQuestions.has(q)) {
          selectedQuestions.delete(q);
        } else {
          selectedQuestions.add(q);
        }
      });
      wrapper.appendChild(btn);
      questionsDiv.appendChild(wrapper);
    });

    evaluateButton.addEventListener("click", () => {
      const result = [];
      for (let [element, props] of Object.entries(elements)) {
        if ([...selectedQuestions].every(q => props.includes(q))) {
          result.push(element);
        }
      }
      resultsDiv.innerHTML = result.length > 0
        ? `<strong>Gefundene Elemente:</strong><br>${result.join(", ")}`
        : "<strong>Keine Elemente passen zu diesen Eigenschaften.</strong>";
    });
  </script>
